.wp-pagenavi a{padding:5px 7px;border:1px solid #FFF;text-decoration:none}
.wp-pagenavi a:hover{border:1px solid #CCC;text-decoration:none;background:#F7F7F7}
.wp-pagenavi span.pages{height:16px;padding:1px 7px 1px 22px !important;padding:2px 7px 2px 22px;line-height:16px;background:url(img/icons.gif) 0 -272px no-repeat}
*+html .wp-pagenavi span.pages{padding:2px 7px 2px 22px !important}
.wp-pagenavi span.current{padding:5px 7px;border:1px solid #F7F7F7;font-weight:bold;background:#F7F7F7}
.wp-pagenavi span.extend{padding:2px 4px}